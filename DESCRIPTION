Package: mixedCCA
Type: Package
Title: Sparse Canonical Correlation Analysis for High-Dimensional Mixed Data
Version: 1.4.5
Date: 2021-03-16
Authors@R: c(
    person(given = "Grace",
           family = "Yoon",
           role = c("aut"),
           email = "gyoon6067@gmail.com",
           comment = c(ORCID = "0000-0003-3263-1352")),
